{% set location = "sidebar" %}
<aside class="site-sidebar {{pageID}}-sidebar" id="site-sidebar" aria-expanded="false">
    <button class="button icon close-sidebar-button" aria-label="Close sidebar"><span aria-hidden="true">{% include "icons/close.svg" %}</span></button>
    {% if pageID.includes("tutoring") %}
    <section class="sidebar-section tutoring-section">
        {% include "partials/components/tutoringNav.njk" %}
    </section>
    {% endif %}
    <section class="sidebar-section sidebar-primary">
        {% include "partials/components/nav.njk" %}
    </section>
</aside>